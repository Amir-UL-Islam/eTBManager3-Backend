<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.ToolBar?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.msh.etbm.desktop.StartupView">
    <children>
        <Pane prefHeight="420.0" prefWidth="522.0">
            <children>
                <ImageView fx:id="logo" fitHeight="150.0" fitWidth="200.0" layoutX="180" layoutY="10" pickOnBounds="true" preserveRatio="true" />

                <Label layoutX="130" layoutY="170" text="e-TB Manager 3">
                    <font>
                        <Font size="36.0" />
                    </font>
                </Label>

                <Button fx:id="btnStart" layoutX="160" layoutY="220.0" minWidth="200" mnemonicParsing="false" onAction="#btnStartAction" prefWidth="200" text="Start e-TB Manager" />

                <Label fx:id="txtStatus" layoutX="30" layoutY="280" text="Label" />

                <ProgressBar fx:id="pbStarting" layoutX="30" layoutY="300" prefHeight="20.0" prefWidth="468.0" progress="0.0" />
            </children>
        </Pane>
    </children>
</AnchorPane>
