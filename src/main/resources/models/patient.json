{
  "name": "patient",
  "table": "patient",
  "fields": [
    {
      "name": "name",
      "label": "${Patient.name}",
      "type": "personName",
      "fieldName": "name",
      "fieldMiddleName": "middleName",
      "fieldLastName": "lastName"
    },
    {
      "name": "birthDate",
      "type": "date",
      "label": "${Patient.birthDate}"
    },
    {
      "name": "gender",
      "type": "string",
      "required": true,
      "label": "${Gender}",
      "options": [
        {
          "id": "MALE",
          "name": "${Gender.MALE}"
        },
        {
          "id": "FEMALE",
          "name": "${Gender.FEMALE}"
        }
      ]
    },
    {
      "name": "motherName",
      "type": "string",
      "max": 100,
      "label": "${Patient.motherName}"
    },
    {
      "name": "customId",
      "type": "string",
      "label": "${form.customId}",
      "max": 50
    }
  ]
}